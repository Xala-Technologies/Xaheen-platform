{
  "name": "user-management",
  "version": "1.0.0",
  "description": "Complete user management with profiles, settings, and RBAC support",
  "type": "shared",
  "context": ["web", "server"],
  "frameworks": ["react", "next", "svelte", "solid"],
  "dependencies": [
    {
      "name": "@tanstack/react-query",
      "version": "^5.0.0",
      "type": "runtime",
      "condition": "includes frameworks 'react'"
    },
    {
      "name": "@tanstack/svelte-query",
      "version": "^5.0.0",
      "type": "runtime",
      "condition": "eq framework 'svelte'"
    },
    {
      "name": "@tanstack/solid-query",
      "version": "^5.0.0",
      "type": "runtime",
      "condition": "eq framework 'solid'"
    },
    {
      "name": "zod",
      "version": "^3.22.0",
      "type": "runtime"
    }
  ],
  "files": [
    {
      "path": "components/user/user-profile.tsx.hbs",
      "type": "template",
      "condition": "and (eq context 'web') (includes frameworks 'react')"
    },
    {
      "path": "components/user/user-settings.tsx.hbs",
      "type": "template",
      "condition": "and (eq context 'web') (includes frameworks 'react')"
    },
    {
      "path": "components/user/user-avatar.tsx.hbs",
      "type": "template",
      "condition": "and (eq context 'web') (includes frameworks 'react')"
    },
    {
      "path": "components/user/role-selector.tsx.hbs",
      "type": "template",
      "condition": "and (eq context 'web') (includes frameworks 'react')"
    },
    {
      "path": "components/user/UserProfile.svelte.hbs",
      "type": "template",
      "condition": "and (eq context 'web') (eq framework 'svelte')"
    },
    {
      "path": "lib/user/user-service.ts.hbs",
      "type": "template",
      "condition": "eq context 'server'"
    },
    {
      "path": "lib/user/rbac.ts.hbs",
      "type": "template",
      "condition": "eq context 'server'"
    },
    {
      "path": "lib/user/permissions.ts.hbs",
      "type": "template"
    },
    {
      "path": "hooks/use-user.ts.hbs",
      "type": "template",
      "condition": "eq context 'web'"
    },
    {
      "path": "hooks/use-permissions.ts.hbs",
      "type": "template",
      "condition": "eq context 'web'"
    },
    {
      "path": "schemas/user.ts.hbs",
      "type": "template"
    },
    {
      "path": "types/user.ts.hbs",
      "type": "template"
    },
    {
      "path": "app/api/users/route.ts.hbs",
      "type": "template",
      "condition": "and (eq context 'server') (eq framework 'next')"
    },
    {
      "path": "app/api/users/[id]/route.ts.hbs",
      "type": "template",
      "condition": "and (eq context 'server') (eq framework 'next')"
    }
  ],
  "variables": {
    "hasUserProfiles": true,
    "hasRBAC": true,
    "hasPermissions": true,
    "hasAvatars": true,
    "hasSettings": true,
    "supportsRoles": true
  },
  "compatibility": {
    "requires": ["ui-base"],
    "suggests": ["auth-better", "notification-toast"],
    "conflicts": []
  }
}