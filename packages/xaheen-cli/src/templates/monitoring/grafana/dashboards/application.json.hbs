{
  "dashboard": {
    "id": null,
    "uid": "{{projectName}}-app-overview",
    "title": "{{projectName}} - Application Overview",
    "description": "Comprehensive application monitoring dashboard for {{projectName}} - Generated by Xaheen CLI",
    "tags": [
      "{{projectName}}",
      "application",
      "monitoring",
      "{{environment}}"
    ],
    "style": "dark",
    "timezone": "browser",
    "editable": true,
    "hideControls": false,
    "graphTooltip": 1,
    
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    
    "timepicker": {
      "refresh_intervals": [
        "5s",
        "10s",
        "30s",
        "1m",
        "5m",
        "15m",
        "30m",
        "1h",
        "2h",
        "1d"
      ],
      "time_options": [
        "5m",
        "15m",
        "1h",
        "6h",
        "12h",
        "24h",
        "2d",
        "7d",
        "30d"
      ]
    },
    
    "refresh": "30s",
    
    "templating": {
      "list": [
        {
          "name": "instance",
          "type": "query",
          "label": "Instance",
          "query": "label_values(up{job=\"{{projectName}}\"}, instance)",
          "current": {
            "text": "All",
            "value": "$__all"
          },
          "includeAll": true,
          "multi": true,
          "refresh": 1,
          "datasource": "Prometheus"
        },
        {
          "name": "interval",
          "type": "interval",
          "label": "Interval",
          "query": "1m,5m,10m,30m,1h",
          "current": {
            "text": "5m",
            "value": "5m"
          },
          "refresh": 2
        }
      ]
    },
    
    "annotations": {
      "list": [
        {
          "name": "Deployments",
          "datasource": "Prometheus", 
          "enable": true,
          "expr": "changes(process_start_time_seconds{job=\"{{projectName}}\"}[5m]) > 0",
          "iconColor": "green",
          "titleFormat": "Deployment",
          "textFormat": "New deployment detected on {{`{{ $labels.instance }}`}}"
        }
      ]
    },
    
    "panels": [
      {
        "id": 1,
        "title": "Service Health Overview",
        "type": "stat",
        "targets": [
          {
            "expr": "up{job=\"{{projectName}}\", instance=~\"$instance\"}",
            "legendFormat": "{{`{{ $labels.instance }}`}}",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 0,
          "y": 0
        },
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {
                "type": "value",
                "options": {
                  "0": { "text": "DOWN", "color": "red" },
                  "1": { "text": "UP", "color": "green" }
                }
              }
            ],
            "thresholds": {
              "steps": [
                { "color": "red", "value": null },
                { "color": "green", "value": 1 }
              ]
            },
            "color": { "mode": "thresholds" },
            "custom": {
              "align": "auto",
              "displayMode": "list"
            }
          }
        },
        "options": {
          "colorMode": "background",
          "graphMode": "none",
          "justifyMode": "center",
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "textMode": "auto"
        }
      },
      
      {
        "id": 2,
        "title": "Request Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(job:http_requests:rate5m{job=\"{{projectName}}\", instance=~\"$instance\"})",
            "legendFormat": "Requests/sec",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 6,
          "y": 0
        },
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "color": { "mode": "palette-classic" },
            "custom": {
              "align": "auto",
              "displayMode": "list"
            },
            "thresholds": {
              "steps": [
                { "color": "green", "value": null },
                { "color": "yellow", "value": 100 },
                { "color": "red", "value": 1000 }
              ]
            }
          }
        },
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "center",
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "textMode": "auto"
        }
      },
      
      {
        "id": 3,
        "title": "Error Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "avg(job:http_requests_error_rate:ratio5m{job=\"{{projectName}}\", instance=~\"$instance\"}) * 100",
            "legendFormat": "Error Rate %",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 12,
          "y": 0
        },
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "color": { "mode": "thresholds" },
            "custom": {
              "align": "auto",
              "displayMode": "list"
            },
            "thresholds": {
              "steps": [
                { "color": "green", "value": null },
                { "color": "yellow", "value": 1 },
                { "color": "orange", "value": 5 },
                { "color": "red", "value": 10 }
              ]
            }
          }
        },
        "options": {
          "colorMode": "background",
          "graphMode": "area",
          "justifyMode": "center",
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "textMode": "auto"
        }
      },
      
      {
        "id": 4,
        "title": "Response Time (P95)",
        "type": "stat",
        "targets": [
          {
            "expr": "avg(job:http_request_duration:p95{job=\"{{projectName}}\", instance=~\"$instance\"}) * 1000",
            "legendFormat": "P95 Latency",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 18,
          "y": 0
        },
        "fieldConfig": {
          "defaults": {
            "unit": "ms",
            "color": { "mode": "thresholds" },
            "custom": {
              "align": "auto",
              "displayMode": "list"
            },
            "thresholds": {
              "steps": [
                { "color": "green", "value": null },
                { "color": "yellow", "value": 200 },
                { "color": "orange", "value": 500 },
                { "color": "red", "value": 1000 }
              ]
            }
          }
        },
        "options": {
          "colorMode": "background",
          "graphMode": "area",
          "justifyMode": "center",
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "textMode": "auto"
        }
      },
      
      {
        "id": 5,
        "title": "Request Rate Over Time",
        "type": "graph",
        "targets": [
          {
            "expr": "sum by (instance) (job:http_requests:rate5m{job=\"{{projectName}}\", instance=~\"$instance\"})",
            "legendFormat": "{{`{{ $labels.instance }}`}}",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 4
        },
        "yAxes": [
          {
            "label": "requests/sec",
            "show": true,
            "min": 0
          },
          {
            "show": true
          }
        ],
        "xAxis": {
          "show": true,
          "mode": "time"
        },
        "lines": true,
        "fill": 1,
        "linewidth": 2,
        "points": false,
        "pointradius": 2,
        "stack": false,
        "percentage": false,
        "legend": {
          "avg": true,
          "current": true,
          "max": true,
          "min": false,
          "show": true,
          "total": false,
          "values": true
        },
        "nullPointMode": "null",
        "tooltip": {
          "shared": true,
          "sort": 0,
          "value_type": "individual"
        }
      },
      
      {
        "id": 6,
        "title": "Response Time Percentiles",
        "type": "graph",
        "targets": [
          {
            "expr": "avg(job:http_request_duration:p50{job=\"{{projectName}}\", instance=~\"$instance\"}) * 1000",
            "legendFormat": "P50",
            "refId": "A"
          },
          {
            "expr": "avg(job:http_request_duration:p90{job=\"{{projectName}}\", instance=~\"$instance\"}) * 1000",
            "legendFormat": "P90",
            "refId": "B"
          },
          {
            "expr": "avg(job:http_request_duration:p95{job=\"{{projectName}}\", instance=~\"$instance\"}) * 1000",
            "legendFormat": "P95",
            "refId": "C"
          },
          {
            "expr": "avg(job:http_request_duration:p99{job=\"{{projectName}}\", instance=~\"$instance\"}) * 1000",
            "legendFormat": "P99",
            "refId": "D"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 4
        },
        "yAxes": [
          {
            "label": "milliseconds",
            "show": true,
            "min": 0
          },
          {
            "show": true
          }
        ],
        "xAxis": {
          "show": true,
          "mode": "time"
        },
        "lines": true,
        "fill": 1,
        "linewidth": 2,
        "points": false,
        "pointradius": 2,
        "stack": false,
        "percentage": false,
        "legend": {
          "avg": true,
          "current": true,
          "max": true,
          "min": false,
          "show": true,
          "total": false,
          "values": true
        },
        "nullPointMode": "null",
        "tooltip": {
          "shared": true,
          "sort": 0,
          "value_type": "individual"
        }
      },
      
      {
        "id": 7,
        "title": "HTTP Status Codes",
        "type": "graph",
        "targets": [
          {
            "expr": "sum by (status) (job:http_requests_by_status:rate5m{job=\"{{projectName}}\", instance=~\"$instance\"})",
            "legendFormat": "{{`{{ $labels.status }}`}}",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 0,
          "y": 12
        },
        "yAxes": [
          {
            "label": "requests/sec",
            "show": true,
            "min": 0
          },
          {
            "show": true
          }
        ],
        "xAxis": {
          "show": true,
          "mode": "time"
        },
        "lines": true,
        "fill": 1,
        "linewidth": 2,
        "points": false,
        "pointradius": 2,
        "stack": true,
        "percentage": false,
        "legend": {
          "avg": false,
          "current": true,
          "max": false,
          "min": false,
          "show": true,
          "total": false,
          "values": true
        },
        "nullPointMode": "null",
        "tooltip": {
          "shared": true,
          "sort": 0,
          "value_type": "individual"
        }
      },
      
      {
        "id": 8,
        "title": "Error Rate Over Time",
        "type": "graph",
        "targets": [
          {
            "expr": "avg by (instance) (job:http_requests_error_rate:ratio5m{job=\"{{projectName}}\", instance=~\"$instance\"}) * 100",
            "legendFormat": "{{`{{ $labels.instance }}`}}",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 8,
          "y": 12
        },
        "yAxes": [
          {
            "label": "error rate %",
            "show": true,
            "min": 0,
            "max": 100
          },
          {
            "show": true
          }
        ],
        "xAxis": {
          "show": true,
          "mode": "time"
        },
        "lines": true,
        "fill": 1,
        "linewidth": 2,
        "points": false,
        "pointradius": 2,
        "stack": false,
        "percentage": false,
        "alert": {
          "conditions": [
            {
              "evaluator": {
                "params": [5],
                "type": "gt"
              },
              "operator": {
                "type": "and"
              },
              "query": {
                "params": ["A", "5m", "now"]
              },
              "reducer": {
                "params": [],
                "type": "avg"
              },
              "type": "query"
            }
          ],
          "executionErrorState": "alerting",
          "for": "5m",
          "frequency": "10s",
          "handler": 1,
          "name": "High Error Rate Alert",
          "noDataState": "no_data",
          "notifications": []
        },
        "thresholds": [
          {
            "colorMode": "critical",
            "fill": true,
            "line": true,
            "op": "gt",
            "value": 5,
            "yaxis": "left"
          }
        ],
        "legend": {
          "avg": true,
          "current": true,
          "max": true,
          "min": false,
          "show": true,
          "total": false,
          "values": true
        },
        "nullPointMode": "null",
        "tooltip": {
          "shared": true,
          "sort": 0,
          "value_type": "individual"
        }
      },
      
      {{#if runtime}}
      {{#eq runtime 'node'}}
      {
        "id": 9,
        "title": "Node.js Memory Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "avg by (instance) (nodejs_heap_size_used_bytes{job=\"{{projectName}}\", instance=~\"$instance\"})",
            "legendFormat": "Heap Used - {{`{{ $labels.instance }}`}}",
            "refId": "A"
          },
          {
            "expr": "avg by (instance) (nodejs_heap_size_total_bytes{job=\"{{projectName}}\", instance=~\"$instance\"})",
            "legendFormat": "Heap Total - {{`{{ $labels.instance }}`}}",
            "refId": "B"
          },
          {
            "expr": "avg by (instance) (nodejs_external_memory_bytes{job=\"{{projectName}}\", instance=~\"$instance\"})",
            "legendFormat": "External Memory - {{`{{ $labels.instance }}`}}",
            "refId": "C"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 16,
          "y": 12
        },
        "yAxes": [
          {
            "label": "bytes",
            "show": true,
            "min": 0
          },
          {
            "show": true
          }
        ],
        "xAxis": {
          "show": true,
          "mode": "time"
        },
        "lines": true,
        "fill": 1,
        "linewidth": 2,
        "points": false,
        "pointradius": 2,
        "stack": false,
        "percentage": false,
        "legend": {
          "avg": true,
          "current": true,
          "max": true,
          "min": false,
          "show": true,
          "total": false,
          "values": true
        },
        "nullPointMode": "null",
        "tooltip": {
          "shared": true,
          "sort": 0,
          "value_type": "individual"
        }
      },
      
      {
        "id": 10,
        "title": "Node.js Event Loop Lag",
        "type": "graph",
        "targets": [
          {
            "expr": "avg by (instance) (nodejs_eventloop_lag_seconds{job=\"{{projectName}}\", instance=~\"$instance\"}) * 1000",
            "legendFormat": "Event Loop Lag - {{`{{ $labels.instance }}`}}",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 6,
          "w": 12,
          "x": 0,
          "y": 20
        },
        "yAxes": [
          {
            "label": "milliseconds",
            "show": true,
            "min": 0
          },
          {
            "show": true
          }
        ],
        "xAxis": {
          "show": true,
          "mode": "time"
        },
        "lines": true,
        "fill": 1,
        "linewidth": 2,
        "points": false,
        "pointradius": 2,
        "stack": false,
        "percentage": false,
        "thresholds": [
          {
            "colorMode": "critical",
            "fill": true,
            "line": true,
            "op": "gt",
            "value": 100,
            "yaxis": "left"
          }
        ],
        "legend": {
          "avg": true,
          "current": true,
          "max": true,
          "min": false,
          "show": true,
          "total": false,
          "values": true
        },
        "nullPointMode": "null",
        "tooltip": {
          "shared": true,
          "sort": 0,
          "value_type": "individual"
        }
      },
      
      {
        "id": 11,
        "title": "Node.js Active Handles & Requests",
        "type": "graph",
        "targets": [
          {
            "expr": "avg by (instance) (nodejs_active_handles{job=\"{{projectName}}\", instance=~\"$instance\"})",
            "legendFormat": "Active Handles - {{`{{ $labels.instance }}`}}",
            "refId": "A"
          },
          {
            "expr": "avg by (instance) (nodejs_active_requests{job=\"{{projectName}}\", instance=~\"$instance\"})",
            "legendFormat": "Active Requests - {{`{{ $labels.instance }}`}}",
            "refId": "B"
          }
        ],
        "gridPos": {
          "h": 6,
          "w": 12,
          "x": 12,
          "y": 20
        },
        "yAxes": [
          {
            "label": "count",
            "show": true,
            "min": 0
          },
          {
            "show": true
          }
        ],
        "xAxis": {
          "show": true,
          "mode": "time"
        },
        "lines": true,
        "fill": 1,
        "linewidth": 2,
        "points": false,
        "pointradius": 2,
        "stack": false,
        "percentage": false,
        "legend": {
          "avg": true,
          "current": true,
          "max": true,
          "min": false,
          "show": true,
          "total": false,
          "values": true
        },
        "nullPointMode": "null",
        "tooltip": {
          "shared": true,
          "sort": 0,
          "value_type": "individual"
        }
      }
      {{/eq}}
      {{/if}}
    ],
    
    "version": 1,
    "schemaVersion": 30,
    "links": [
      {
        "icon": "external link",
        "tags": [],
        "targetBlank": true,
        "title": "Infrastructure Dashboard",
        "tooltip": "View infrastructure metrics",
        "type": "dashboards",
        "url": "/d/{{projectName}}-infra-overview"
      }
      {{#if features.includes('distributed-tracing')}}
      ,{
        "icon": "external link", 
        "tags": [],
        "targetBlank": true,
        "title": "Distributed Tracing",
        "tooltip": "View distributed traces",
        "type": "link",
        "url": "{{#if features.includes('jaeger')}}/d/jaeger{{else if features.includes('tempo')}}/d/tempo{{else}}/d/jaeger{{/if}}"
      }
      {{/if}}
      {{#if features.includes('loki')}}
      ,{
        "icon": "external link",
        "tags": [],
        "targetBlank": true, 
        "title": "Application Logs",
        "tooltip": "View application logs",
        "type": "link",
        "url": "/d/{{projectName}}-logs"
      }
      {{/if}}
    ]
  }
}