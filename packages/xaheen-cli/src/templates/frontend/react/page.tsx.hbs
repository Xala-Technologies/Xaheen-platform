 *     name: "Ole Hansen",
 *     role: "Government Administrator",
 *     clearanceLevel: "{{mcpNSMLevel this}}"
 *   }}
 *   locale="nb-NO"
 *   params={{ id: "123" }}
 *   searchParams={{ tab: "overview" }}
 * />
 */