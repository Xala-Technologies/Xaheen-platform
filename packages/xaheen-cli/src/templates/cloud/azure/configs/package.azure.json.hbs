{
  "name": "azure-services",
  "version": "1.0.0",
  "description": "Azure services integration package with comprehensive service coverage",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/index.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "azure:login": "az login",
    "azure:deploy": "npm run build && node scripts/deploy.js",
    "azure:functions:start": "func start",
    "azure:functions:deploy": "func azure functionapp publish your-function-app-name"
  },
  "dependencies": {
    "@azure/identity": "^4.0.1",
    "@azure/keyvault-secrets": "^4.8.0",
    "@azure/keyvault-keys": "^4.8.0",
    "@azure/keyvault-certificates": "^4.8.0",
    "@azure/service-bus": "^7.9.4",
    "@azure/storage-blob": "^12.17.0",
    "@azure/storage-file-share": "^12.17.0",
    "@azure/storage-queue": "^12.15.0",
    "@azure/cosmos": "^4.0.0",
    "@azure/data-tables": "^13.2.2",
    "@azure/search-documents": "^12.0.0",
    "@azure/communication-common": "^2.3.0",
    "@azure/communication-sms": "^1.1.0",
    "@azure/communication-email": "^1.0.0",
    "@azure/eventgrid": "^5.4.0",
    "@azure/event-hubs": "^5.11.4",
    "@azure/notification-hubs": "^1.1.2",
    "@azure/monitor-query": "^1.2.0",
    "@azure/arm-resources": "^5.2.0",
    "@azure/arm-storage": "^18.1.0",
    "@azure/arm-sql": "^10.1.0",
    "@azure/arm-keyvault": "^3.1.0",
    "@azure/arm-servicebus": "^6.1.0",
    "@azure/arm-cognitiveservices": "^7.5.0",
    "@azure/arm-appservice": "^14.1.0",
    "@azure/arm-containerregistry": "^10.1.0",
    "@azure/arm-containerservice": "^19.7.0",
    "@azure/arm-network": "^33.3.0",
    "@azure/arm-monitor": "^8.1.0",
    "@azure/arm-automation": "^4.1.0",
    "msal-node": "^2.6.6",
    "jsonwebtoken": "^9.0.2",
    "jwks-client": "^3.0.1",
    "redis": "^4.6.12",
    "tedious": "^16.7.1",
    "typeorm": "^0.3.20",
    {{#if enableAiServices}}
    {{#each aiServices}}
    {{#if (eq this 'openai')}}
    "openai": "^4.28.0",
    "@azure/openai": "^1.0.0-beta.12",
    {{/if}}
    {{#if (eq this 'cognitive-services')}}
    "@azure/cognitiveservices-computervision": "^8.2.0",
    "@azure/cognitiveservices-textanalytics": "^5.0.0",
    "@azure/cognitiveservices-face": "^5.0.0",
    {{/if}}
    {{#if (eq this 'document-intelligence')}}
    "@azure/ai-form-recognizer": "^5.0.0",
    {{/if}}
    {{#if (eq this 'speech')}}
    "microsoft-cognitiveservices-speech-sdk": "^1.34.1",
    {{/if}}
    {{#if (eq this 'language')}}
    "@azure/ai-language-text": "^1.1.0",
    "@azure/ai-language-conversations": "^1.1.0",
    {{/if}}
    {{#if (eq this 'translator')}}
    "@azure/ai-translation-text": "^1.0.0",
    {{/if}}
    {{#if (eq this 'bot-service')}}
    "botbuilder": "^4.21.1",
    "botframework-connector": "^4.21.1",
    {{/if}}
    {{/each}}
    {{/if}}
    "dotenv": "^16.4.5",
    "winston": "^3.11.0",
    "applicationinsights": "^2.9.5",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "cors": "^2.8.5",
    "compression": "^1.7.4",
    "rate-limiter-flexible": "^4.0.1",
    "joi": "^17.12.1",
    "class-validator": "^0.14.1",
    "class-transformer": "^0.5.1",
    "uuid": "^9.0.1",
    "date-fns": "^3.3.1",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@types/node": "^20.11.19",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/compression": "^1.7.5",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/uuid": "^9.0.8",
    "@types/lodash": "^4.14.202",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.2.5",
    "typescript": "^5.3.3",
    "ts-node": "^10.9.2",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.12",
    "ts-jest": "^29.1.2",
    "supertest": "^6.3.4",
    "@types/supertest": "^6.0.2",
    "rimraf": "^5.0.5",
    "nodemon": "^3.0.3",
    "azure-functions-core-tools": "^4.0.5455",
    "@azure/functions": "^4.4.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/azure-services.git"
  },
  "keywords": [
    "azure",
    "cloud",
    "microservices",
    "serverless",
    "functions",
    "service-bus",
    "sql-database",
    "key-vault",
    "ai-services",
    "cognitive-services",
    "openai",
    "typescript"
  ],
  "author": "Your Organization",
  "license": "MIT",
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": ["<rootDir>/src", "<rootDir>/tests"],
    "testMatch": ["**/__tests__/**/*.ts", "**/?(*.)+(spec|test).ts"],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.d.ts",
      "!src/index.ts"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": ["text", "lcov", "html"],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  },
  "eslintConfig": {
    "extends": [
      "@typescript-eslint/recommended",
      "prettier"
    ],
    "parser": "@typescript-eslint/parser",
    "plugins": ["@typescript-eslint"],
    "rules": {
      "@typescript-eslint/no-unused-vars": "error",
      "@typescript-eslint/explicit-function-return-type": "warn",
      "@typescript-eslint/no-explicit-any": "error",
      "@typescript-eslint/no-empty-function": "warn",
      "prefer-const": "error",
      "no-var": "error"
    }
  },
  "prettier": {
    "semi": true,
    "trailingComma": "es5",
    "singleQuote": true,
    "printWidth": 100,
    "tabWidth": 2,
    "useTabs": false
  }
}