{
  "name": "@xaheen/design-system",
  "version": "1.0.0",
  "description": "Universal design system that works across all platforms - React, Vue, Angular, Svelte, React Native, and more",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./registry": {
      "types": "./registry/index.ts",
      "import": "./registry/index.ts",
      "require": "./registry/index.ts"
    },
    "./registry/universal": {
      "types": "./registry/universal-index.ts",
      "import": "./registry/universal-index.ts",
      "require": "./registry/universal-index.ts"
    },
    "./react": {
      "types": "./registry/platforms/react/index.ts",
      "import": "./registry/platforms/react/index.ts",
      "require": "./registry/platforms/react/index.ts"
    },
    "./react/*": {
      "types": "./registry/platforms/react/*.tsx",
      "import": "./registry/platforms/react/*.tsx",
      "require": "./registry/platforms/react/*.tsx"
    },
    "./vue": {
      "types": "./registry/platforms/vue/index.ts",
      "import": "./registry/platforms/vue/index.ts",
      "require": "./registry/platforms/vue/index.ts"
    },
    "./vue/*": {
      "types": "./registry/platforms/vue/*.vue",
      "import": "./registry/platforms/vue/*.vue",
      "require": "./registry/platforms/vue/*.vue"
    },
    "./angular": {
      "types": "./registry/platforms/angular/index.ts",
      "import": "./registry/platforms/angular/index.ts",
      "require": "./registry/platforms/angular/index.ts"
    },
    "./angular/*": {
      "types": "./registry/platforms/angular/*.ts",
      "import": "./registry/platforms/angular/*.ts",
      "require": "./registry/platforms/angular/*.ts"
    },
    "./svelte": {
      "types": "./registry/platforms/svelte/index.ts",
      "import": "./registry/platforms/svelte/index.ts",
      "require": "./registry/platforms/svelte/index.ts"
    },
    "./svelte/*": {
      "types": "./registry/platforms/svelte/*.svelte",
      "import": "./registry/platforms/svelte/*.svelte",
      "require": "./registry/platforms/svelte/*.svelte"
    },
    "./react-native": {
      "types": "./registry/platforms/react-native/index.ts",
      "import": "./registry/platforms/react-native/index.ts",
      "require": "./registry/platforms/react-native/index.ts"
    },
    "./react-native/*": {
      "types": "./registry/platforms/react-native/*.tsx",
      "import": "./registry/platforms/react-native/*.tsx",
      "require": "./registry/platforms/react-native/*.tsx"
    },
    "./radix": {
      "types": "./registry/platforms/radix/index.ts",
      "import": "./registry/platforms/radix/index.ts",
      "require": "./registry/platforms/radix/index.ts"
    },
    "./radix/*": {
      "types": "./registry/platforms/radix/*.tsx",
      "import": "./registry/platforms/radix/*.tsx",
      "require": "./registry/platforms/radix/*.tsx"
    },
    "./headless-ui": {
      "types": "./registry/platforms/headless-ui/index.ts",
      "import": "./registry/platforms/headless-ui/index.ts",
      "require": "./registry/platforms/headless-ui/index.ts"
    },
    "./headless-ui/*": {
      "types": "./registry/platforms/headless-ui/*.tsx",
      "import": "./registry/platforms/headless-ui/*.tsx",
      "require": "./registry/platforms/headless-ui/*.tsx"
    },
    "./vanilla": {
      "types": "./registry/platforms/vanilla/index.ts",
      "import": "./registry/platforms/vanilla/index.ts",
      "require": "./registry/platforms/vanilla/index.ts"
    },
    "./vanilla/*": {
      "types": "./registry/platforms/vanilla/*.js",
      "import": "./registry/platforms/vanilla/*.js",
      "require": "./registry/platforms/vanilla/*.js"
    },
    "./electron": {
      "types": "./registry/platforms/electron/index.ts",
      "import": "./registry/platforms/electron/index.ts",
      "require": "./registry/platforms/electron/index.ts"
    },
    "./electron/*": {
      "types": "./registry/platforms/electron/*.tsx",
      "import": "./registry/platforms/electron/*.tsx",
      "require": "./registry/platforms/electron/*.tsx"
    },
    "./ionic": {
      "types": "./registry/platforms/ionic/index.ts",
      "import": "./registry/platforms/ionic/index.ts",
      "require": "./registry/platforms/ionic/index.ts"
    },
    "./ionic/*": {
      "types": "./registry/platforms/ionic/*.tsx",
      "import": "./registry/platforms/ionic/*.tsx",
      "require": "./registry/platforms/ionic/*.tsx"
    },
    "./nextjs": {
      "types": "./registry/platforms/react/index.ts",
      "import": "./registry/platforms/react/index.ts",
      "require": "./registry/platforms/react/index.ts"
    },
    "./nextjs/*": {
      "types": "./registry/platforms/react/*.tsx",
      "import": "./registry/platforms/react/*.tsx",
      "require": "./registry/platforms/react/*.tsx"
    },
    "./nuxt": {
      "types": "./registry/platforms/vue/index.ts",
      "import": "./registry/platforms/vue/index.ts",
      "require": "./registry/platforms/vue/index.ts"
    },
    "./nuxt/*": {
      "types": "./registry/platforms/vue/*.vue",
      "import": "./registry/platforms/vue/*.vue",
      "require": "./registry/platforms/vue/*.vue"
    },
    "./sveltekit": {
      "types": "./registry/platforms/svelte/index.ts",
      "import": "./registry/platforms/svelte/index.ts",
      "require": "./registry/platforms/svelte/index.ts"
    },
    "./sveltekit/*": {
      "types": "./registry/platforms/svelte/*.svelte",
      "import": "./registry/platforms/svelte/*.svelte",
      "require": "./registry/platforms/svelte/*.svelte"
    },
    "./expo": {
      "types": "./registry/platforms/react-native/index.ts",
      "import": "./registry/platforms/react-native/index.ts",
      "require": "./registry/platforms/react-native/index.ts"
    },
    "./expo/*": {
      "types": "./registry/platforms/react-native/*.tsx",
      "import": "./registry/platforms/react-native/*.tsx",
      "require": "./registry/platforms/react-native/*.tsx"
    },
    "./cli": {
      "types": "./registry/cli/component-cli.ts",
      "import": "./registry/cli/component-cli.ts",
      "require": "./registry/cli/component-cli.ts"
    },
    "./recipes": {
      "types": "./registry/recipes/component-recipes.ts",
      "import": "./registry/recipes/component-recipes.ts",
      "require": "./registry/recipes/component-recipes.ts"
    },
    "./tokens": {
      "types": "./registry/core/universal-tokens.ts",
      "import": "./registry/core/universal-tokens.ts",
      "require": "./registry/core/universal-tokens.ts"
    }
  },
  "files": [
    "dist",
    "registry",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean && pnpm run build:registry",
    "build:registry": "pnpm exec tsx scripts/build-registry.ts",
    "build:components": "pnpm exec tsx scripts/build-components.ts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "lint": "eslint src registry --ext .ts,.tsx",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "test:platforms": "vitest --run registry/tests/platform-compatibility.test.ts",
    "test:generation": "vitest --run registry/tests/component-generation.test.ts",
    "prebuild-storybook": "pnpm run build",
    "storybook": "pnpm run build && storybook dev -p 6006",
    "build-storybook": "storybook build",
    "type-check": "tsc --noEmit",
    "registry": "pnpm exec tsx scripts/build-registry.ts",
    "generate": "pnpm exec tsx scripts/generate-component.ts",
    "validate": "pnpm run type-check && pnpm run lint && pnpm run test:platforms",
    "prepack": "pnpm run build && pnpm run validate"
  },
  "keywords": [
    "design-system",
    "universal",
    "multi-platform",
    "react",
    "vue",
    "angular",
    "svelte",
    "react-native",
    "typescript",
    "tailwind",
    "accessibility",
    "wcag",
    "radix-ui",
    "headless-ui",
    "web-components",
    "norwegian",
    "nsm"
  ],
  "author": "Xaheen Technologies",
  "license": "MIT",
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "dependencies": {
    "@heroicons/react": "^2.0.18",
    "@hookform/resolvers": "^3.3.2",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-tooltip": "^1.0.7",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "lucide-react": "^0.294.0",
    "react-hook-form": "^7.48.2",
    "tailwind-merge": "^3.3.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^8.6.14",
    "@storybook/addon-docs": "^8.6.14",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-links": "^8.6.14",
    "@storybook/addon-onboarding": "^8.6.14",
    "@storybook/addon-viewport": "^8.6.14",
    "@storybook/react": "8.6.14",
    "@storybook/react-vite": "^8.6.14",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.56.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.8.0",
    "jest-axe": "^8.0.0",
    "postcss": "^8.4.32",
    "storybook": "8.6.14",
    "tailwindcss": "^3.3.6",
    "tsup": "^8.0.1",
    "tsx": "^4.0.0",
    "typescript": "^5.3.3",
    "vite": "^5.4.19",
    "vitest": "^1.1.0"
  }
}
