        }
      ]
    },
    {
      "name": "theme-switcher",
      "type": "registry:component",
      "title": "Theme Switcher",
      "description": "Theme switching component with system preference detection",
      "category": "theme-components",
      "platforms": ["react", "nextjs", "vue", "angular", "svelte", "react-native", "electron", "ionic", "vanilla", "headless-ui", "radix"],
      "registryDependencies": ["utils", "button"],
      "files": [
        {
          "path": "components/theme-switcher/theme-switcher.tsx",
          "type": "registry:component"
        }
      ]
    },
    {
      "name": "utils",
      "type": "registry:utils",
      "title": "Utilities",
      "description": "Core utility functions",
      "platforms": ["react", "nextjs", "vue", "angular", "svelte", "react-native", "electron", "ionic", "vanilla", "headless-ui", "radix"],
      "dependencies": ["clsx", "tailwind-merge"],
      "files": [
        {
          "path": "utils/cn.ts",
          "type": "registry:utils"
        }
      ]
    },
    {
      "name": "use-responsive",
      "type": "registry:hook",
      "title": "useResponsive",
      "description": "Responsive design hook with breakpoint detection",
      "platforms": ["react", "nextjs", "vue", "angular", "svelte", "react-native", "electron", "ionic", "vanilla", "headless-ui", "radix"],
      "files": [
        {
          "path": "hooks/use-responsive.ts",
          "type": "registry:hook"
        }
      ]
    },
    {
      "name": "use-accessibility",
      "type": "registry:hook",
      "title": "useAccessibility",
      "description": "WCAG AAA compliance hooks",
      "platforms": ["react", "nextjs", "vue", "angular", "svelte", "react-native", "electron", "ionic", "vanilla", "headless-ui", "radix"],
      "files": [
        {
          "path": "hooks/use-accessibility.ts",
          "type": "registry:hook"
        }
      ]
    },
    {
      "name": "xaheen-theme",
      "type": "registry:style",
      "title": "Xaheen Theme",
      "description": "Professional design system theme with Norwegian standards",
      "extends": "none",
      "dependencies": ["tailwind-merge", "clsx"],
      "registryDependencies": ["utils"],
      "files": [
        {
          "path": "styles/globals.css",
          "type": "registry:style",
          "target": "~/styles/globals.css"
        }
      ],
      "cssVars": {
        "theme": {
          "font-sans": "Inter, -apple-system, sans-serif",
          "font-mono": "JetBrains Mono, monospace"
        },
        "light": {
          "primary": "#3B82F6",
          "secondary": "#64748B",
          "success": "#10B981",
          "warning": "#F59E0B",
          "error": "#EF4444",
          "background": "#FFFFFF",
          "foreground": "#0F172A",
          "border": "#E2E8F0",
          "ring": "#3B82F6"
        },
        "dark": {
          "primary": "#60A5FA",
          "secondary": "#94A3B8",
          "success": "#34D399",
          "warning": "#FBBF24",
          "error": "#F87171",
          "background": "#0F172A",
          "foreground": "#F8FAFC",
          "border": "#334155",
          "ring": "#60A5FA"
        }
      },
      "config": {
        "tailwind": {
          "extend": {
            "spacing": {
              "13": "3.25rem",
              "15": "3.75rem",
              "128": "32rem",
              "144": "36rem"
            }
          }
        }
      }
    },
    {
      "name": "login-form",
      "type": "registry:block",
      "title": "Login Form",
      "description": "Accessible login form with Norwegian language support",
      "category": "patterns",
      "nsm": {
        "classification": "RESTRICTED",
        "wcagLevel": "AAA",
        "norwegianOptimized": true
      },
      "platforms": ["react", "nextjs", "vue", "angular", "svelte", "react-native", "electron", "ionic", "vanilla", "headless-ui", "radix"],
      "registryDependencies": ["button", "input", "card"],
      "files": [
        {
          "path": "blocks/login-form/login-form.tsx",
          "type": "registry:component"
        },
        {
          "path": "blocks/login-form/page.tsx",
          "type": "registry:page",
          "target": "app/login/page.tsx"
        }
      ]
    }
